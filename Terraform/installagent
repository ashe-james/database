        /u01/app/oracle/agent/agent_13.3.0.0.0/root.sh



RUN {
    BACKUP AS COMPRESSED BACKUPSET INCREMENTAL LEVEL 0 DATABASE FORMAT '/backup/rman/%T_%d_%p_%s.0.bak';
    BACKUP AS BACKUPSET ARCHIVELOG ALL FORMAT '/backup/rman/%T_%d_%p_%s.arc' delete input;
    CROSSCHECK BACKUP;
    DELETE NOPROMPT EXPIRED BACKUP;
    REPORT OBSOLETE;
    DELETE NOPROMPT OBSOLETE;
}